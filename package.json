      "description": "Allow to run commands in terminal.",
      "versions": {
        "0.14.5": "provideRunInTerminal"
      }
    }
  },
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true
}
